MAIN    START
        LD      GR1,A   ;GR1 = 3
        ADDA    GR1,B   ;GR1 = GR1 + B
        ST      GR1,X   ;C = GR1
        ADDA    GR1,MOJI
        ST      GR1,S
        OUT     S,LEN
        RET
A       DC      3
B       DC      4
X       DS      1
S       DS      1
MOJI    DC      #0030
LEN     DC      1
        END
